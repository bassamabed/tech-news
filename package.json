{
  "name": "baltex",
  "version": "0.1.0",
  "private": true,
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Emmancipate Takunda Musemwa",
  "dependencies": {
    "axios": "0.18.0",
    "bootstrap": "4.1.2",
    "classnames": "2.2.6",
    "connected-react-router": "4.3.0",
    "cookie-parser": "1.4.3",
    "cross-env": "^5.2.0",
    "forcedomain": "1.0.0",
    "ignore-styles": "5.0.1",
    "immutability-helper": "2.7.1",
    "js-cookie": "2.2.0",
    "md5-file": "4.0.0",
    "morgan": "1.9.0",
    "node-sass-chokidar": "1.3.0",
    "npm-run-all": "4.1.3",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-frontload": "1.0.1",
    "react-helmet": "5.2.0",
    "react-intl": "2.4.0",
    "react-intl-redux": "2.0.0",
    "react-loadable": "5.4.0",
    "react-notification-system": "0.2.17",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "reactstrap": "6.3.0",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "extract-messages": "cross-env NODE_ENV=development babel ./src  --out-file /dev/null",
    "manage:translations": "node ./translationRunner",
    "serve": "cross-env NODE_ENV=production npm run build && node ./server/index.js",
    "lint-fix": "eslint --fix .eslintrc src"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-plugin-react-intl": "2.4.0",
    "babel-preset-react-app": "3.1.2",
    "eslint": "5.2.0",
    "eslint-plugin-react": "7.10.0",
    "react-intl-translations-manager": "5.0.3"
  }
}
